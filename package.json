{
	"name": "@manchtools/svelte-cookie-consent",
	"version": "0.2.0",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/manchtools/svelte-cookie-consent.git"
	},
	"keywords": [
		"svelte",
		"cookie",
		"cookies",
		"banner",
		"consent",
		"svelte-cookie-consent",
		"gdpr",
		"complience"
	],
	"bugs": {
		"url": "https://github.com/manchtools/svelte-cookie-consent/issues",
		"email": "paul.dotterer@icloud.com"
	},
	"author": {
		"name": "Paul Dotterer",
		"email": "paul.dotterer@icloud.com"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@sveltejs/package": "^1.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"js-cookie": "^3.0.1",
		"svelte": "^3.54.0"
	}
}
